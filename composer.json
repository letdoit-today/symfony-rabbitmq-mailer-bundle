{
    "name": "letdoittoday/rabbitmq-mailer-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "jms/serializer-bundle": "*",
        "letdoittoday/rabbitmq-bundle": "1.*",
        "symfony/mailer": "5.0.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DIT\\RabbitMQMailerBundle\\": ""
        },
        "exclude-from-classmap": [
            "Resources/",
            "Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
